{
  "name": "pluralsight-reusable-components-cra",
  "version": "0.1.14",
  "main": "./lib/index.js",
  "module": "./lib/index.es.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "author": {
    "name": "Cory House",
    "email": "cory@reactjsconsulting.com",
    "url": "http://reactjsconsulting.com/"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "components",
    "react",
    "react-component",
    "toolkit",
    "pluralsight"
  ],
  "homepage": "http://coryhouse.github.io/pluralsight-reusable-components-cra",
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-cli": "^6.23.0",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-es2015-modules-umd": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "chokidar": "^1.6.1",
    "connect-history-api-fallback": "1.3.0",
    "cpx": "^1.5.0",
    "cross-env": "^3.1.4",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "enzyme": "^2.7.1",
    "eslint": "3.8.1",
    "eslint-config-react-app": "^0.5.2",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "highlight.js": "^9.10.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "npm-run-all": "^4.0.2",
    "object-assign": "4.1.1",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dev-utils": "^0.5.1",
    "react-docgen": "^2.13.0",
    "react-dom": "^15.4.2",
    "react-highlight": "^0.9.0",
    "react-test-renderer": "^15.4.2",
    "react-title-component": "^1.0.1",
    "recursive-readdir": "2.1.1",
    "rimraf": "^2.6.1",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "dependencies": {
    "styled-components": "^1.4.4"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel gen:docs start:docs",
    "start:docs": "node scripts/start.js",
    "gen:docs": "node scripts/generateComponentData.js",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:css",
    "build:commonjs": "cross-env NODE_ENV=release babel ./src/components --out-dir ./lib --ignore spec.js",
    "build:es": "cross-env BABEL_ENV=es babel ./src/components --out-file ./lib/index.es.js --ignore spec.js",
    "build:umd": "cross-env BABEL_ENV=umd babel ./src/components --out-file ./lib/index.umd.js --ignore spec.js",
    "build:css": "cpx \"./src/components/**/*.css\" ./lib",
    "build:docs": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "prepublish": "npm run build:lib",
    "predeploy:docs": "npm run build:docs",
    "deploy:docs": "gh-pages -d docs"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^ps-ui(.*)$": "<rootDir>/src/components$1"
    }
  },
  "babel": {
    "presets": [
      "./config/pluralsight-babel-preset",
      "stage-1",
      "react"
    ],
    "env": {
      "umd": {
        "plugins": [
          "transform-es2015-modules-umd"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
